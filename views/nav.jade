extends ./layout.jade

block navigation
  header
    nav.mainnav
      h1.site-logo
        a.logo(href="/")
          img(alt="Respect.ly" id="mlogo" src="/images/wl.png")
      ul.left
        li
          a(href='#') ABOUT
        li
          a(href="#") START
      audio#audio(loop autoplay src="/music/11.mp3")
      ul.right
        li
          a(href="#" data-toggle="modal" data-target="#join") JOIN
        li
          a(href="#" data-toggle="modal" data-target="#login") LOGIN
        li
          a(href="/")#music
            span.glyphicon.glyphicon-pause(aria-hidden="true")
  .subheader
    nav.subnav
      h1.site-logo
        a.logo(href="/")
          img(alt="Respect.ly" id="slogo" src="/images/vl.png")
      ul.left
        li
          a(href='#') ABOUT
        li
          a(href="#") START
      ul.right
        li
          a(href="#" data-toggle="modal" data-target="#join") JOIN
        li
          a(href="#" data-toggle="modal" data-target="#login") LOGIN
        li
          a(href="/")#music
            span.glyphicon.glyphicon-pause(aria-hidden="true")

